/* cd-video */
.cd-video, .cd-video-detail {
  width: 100%;
  height: 380rpx;
}
/* cd-cont */
@import "../../components/complist/complist.wxss";
.cd-cont {
  width: 100%;
  background-color: #fff;
}
.cd-up-div {
  padding: 10rpx 20rpx;
}
.cd-up-title {
  font-size: 28rpx;
  padding: 10rpx 0 10rpx 36rpx;
  position: relative;
}
.cd-up-title::after {
  position: absolute;
  top: 24rpx;
  left: 20rpx;
  content: '';
  width: 10rpx;
  height: 10rpx;
  background-color: #36037C;
  border-radius: 50%;
}


/* cd-none */
.cd-none {
  margin-top: 40rpx;
  text-align: center;  
}
.cd-none-text {
  font-size: 28rpx;
  color: #666;
}
